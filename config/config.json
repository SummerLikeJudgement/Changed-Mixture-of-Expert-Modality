{
  "datasetCommonParams": {
    "dataset_root_dir": "./dataset",
    "biovid": {
      "unaligned": {
        "featurePath": "BIOVID/unaligned.pkl",
        "feature_dims": [26, 39, 35],
        "train_samples": 8700,
        "num_classes": 5,
